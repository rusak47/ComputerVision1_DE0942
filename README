git lfs track "*.7z"
git lfs track "*.zip"
git lfs track "*.ipynb"
git add .gitattributes
git commit -m "Track big files with Git LFS"
git lfs migrate import --include="*.zip,*.7z,*.jpg,*.png,*.ipynb"
